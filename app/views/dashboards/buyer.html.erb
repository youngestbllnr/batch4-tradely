<% if user_signed_in?%>   
    <%= link_to 'Sign out', destroy_user_session_path , method: :delete %>
    
    <h1>Welcome, <%= current_user.role%></h1>
    <p>fname: <%= current_user.firstname%>/email: <%= current_user.email%></p>

    <% @array.each do |d| %>
        <hr>
        <p><%= link_to 'buy' %></p>
        <p><%= d.company_name %></p>  
        <p>avg_total_volume = <%= d.avg_total_volume %></p>
        <p>latest_price = <%= d.latest_price %></p>
        <p>change = <%= d.change %></p>
        <p>previous_close = <%= d.previous_close %></p>
    <%end%>
<%end%>