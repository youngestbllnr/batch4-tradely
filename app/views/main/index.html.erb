<% if user_signed_in?%>   
    <%= link_to 'Sign out', destroy_user_session_path , method: :delete %>
    
    <h1>Welcome, <%= current_user.role%></h1>
    <p>fname: <%= current_user.firstname%>/email: <%= current_user.email%></p>
    
    <p><%= @MSFT.company_name %></p>  
    <p>avg_total_volume = <%= @MSFT.avg_total_volume %></p>
    <p>latest_price = <%= @MSFT.latest_price %></p>
    <p>change_percent = <%= @MSFT.change_percent %></p>
    <p>change_percent_s = <%= @MSFT.change_percent_s %></p>
    <p><%= @MSFT %></p>

<% else%>
    
    <%= link_to 'Register', new_user_registration_path %>
    <%= link_to 'Sign in', new_user_session_path %>

<%end%>
